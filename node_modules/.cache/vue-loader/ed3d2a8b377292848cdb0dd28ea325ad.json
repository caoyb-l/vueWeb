;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoTA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"CreateFace.vue","sourceRoot":"src/views/face/modules","sourcesContent":["<template>\n  <!-- 新增弹框 -->\n  <a-modal\n    title=\"新建\"\n    style=\"top: 20px;\"\n    :width=\"600\"\n    :destroyOnClose=\"true\"\n    v-model=\"visible\"\n    @ok=\"handleSubmit\"\n    v-preventReClick\n    :maskClosable = \"false\"\n  >\n    <a-form :form=\"form\">\n      <a-form-item\n        label=\"姓名\"\n        :labelCol=\"labelCol\"\n        :wrapperCol=\"wrapperCol\">\n        <a-input\n          v-decorator=\"[\n            'name',\n            {rules: [{ required: true, message: '请输入' }]}\n          ]\"\n          placeholder=\"姓名\" />\n      </a-form-item>\n      <a-form-item\n        label=\"人脸描述\"\n        :labelCol=\"labelCol\"\n        :wrapperCol=\"wrapperCol\">\n        <a-textarea\n          v-decorator=\"[\n            'Descn',\n            {rules: [{ required:true, message: '请输入' }]}\n          ]\"\n          row=\"2\">\n        </a-textarea>\n      </a-form-item>\n      <a-form-item\n        label=\"人脸应答内容\"\n        :labelCol=\"labelCol\"\n        :wrapperCol=\"wrapperCol\">\n        <a-textarea\n          v-decorator=\"[\n            'msg',\n            {rules: [{ required: true, message: '请输入' }]}\n          ]\"\n          row=\"2\">\n        </a-textarea>\n      </a-form-item>\n      <a-form-item label=\"地图\" :labelCol=\"labelCol\" :wrapperCol=\"wrapperCol\">\n        <a-select\n          showSearch\n          placeholder=\"如需使用导航请先选择地图\"\n          optionFilterProp=\"children\"\n          v-decorator=\"['mapId']\"\n        >\n          <a-select-option v-for=\"(item ,key) in mapList\" :key=\"key\" :value=\"item.id\">\n            {{ item.mapName }}\n          </a-select-option>\n        </a-select>\n      </a-form-item>\n      <a-form-item\n        label=\"动作集\"\n        :labelCol=\"labelCol\"\n        :wrapperCol=\"wrapperCol\">\n        <a-select\n          showSearch\n          placeholder=\"选择动作集\"\n          optionFilterProp=\"children\"\n          @change=\"changeCmd($event)\"\n          v-decorator=\"[\n            'cmd',\n            {initialValue: cmd}\n          ]\"\n        >\n          <a-select-option v-for=\"(item ,key) in cmdList\" :key=\"key\" :value=\"item.id\">\n            {{ item.name }}\n          </a-select-option>\n        </a-select>\n        <a-button type=\"primary\" @click=\"addCmd()\">新增一个动作集</a-button>\n        <a-divider type=\"vertical\"/>\n        <a-button id=\"chgCmd\" type=\"primary\" @click=\"editCmd()\" v-if=\"showEditCmd\">修改当前动作集</a-button>\n      </a-form-item>\n\n      <a-form-item\n        label=\"年龄\"\n        :labelCol=\"labelCol\"\n        :wrapperCol=\"wrapperCol\">\n        <a-input\n          v-decorator=\"[\n            'age',\n            {rules: [{ message: '请输入' }]}\n          ]\"\n          placeholder=\"年龄\" />\n      </a-form-item>\n      <a-form-item\n        label=\"性别\"\n        :labelCol=\"labelCol\"\n        :wrapperCol=\"wrapperCol\">\n        <a-select\n          placeholder=\"选择性别\"\n          optionFilterProp=\"children\"\n          v-decorator=\"[\n            'sex',\n            {rules: [{ message: '请选择' }]}\n          ]\"\n        >\n          <a-select-option value=\"1\">男</a-select-option>\n          <a-select-option value=\"2\">女</a-select-option>\n        </a-select>\n      </a-form-item>\n      <!-- 人脸上传文件 -->\n      <a-form-item\n        label=\"上传\"\n        :labelCol=\"labelCol\"\n        :wrapperCol=\"wrapperCol\">\n        <a-upload\n          :action= \"url\"\n          listType=\"picture-card\"\n          :fileList=\"fileList\"\n          @preview=\"handlePreview\"\n          @change=\"handleChange\"\n        >\n          <div v-if=\"fileList.length < 1\">\n            <a-icon type=\"plus\" />\n            <div class=\"ant-upload-text\">Upload</div>\n          </div>\n        </a-upload>\n        <a-modal :visible=\"previewVisible\" :footer=\"null\" @cancel=\"handleCancel\">\n          <img alt=\"example\" style=\"width: 100%\" :src=\"previewImage\" />\n        </a-modal>\n      </a-form-item>\n    </a-form>\n    <cmd-list @getCmdId=\"getCmdId\" ref=\"cmd\"></cmd-list>\n  </a-modal>\n</template>\n\n<script>\nimport { addData, queryData } from '@/api/common'\nimport CmdList from '../../Cmd'\nimport { uploadUrl } from '@/api/upload'\nexport default {\n  components: {\n    CmdList\n  },\n  data () {\n    return {\n      labelCol: {\n        xs: { span: 24 },\n        sm: { span: 7 }\n      },\n      wrapperCol: {\n        xs: { span: 24 },\n        sm: { span: 13 }\n      },\n      visible: false,\n      confirmLoading: false,\n      form: this.$form.createForm(this),\n      previewVisible: false,\n      previewImage: '',\n      fileList: [],\n      fileName: '',\n      cmdList: [],\n      photo: '',\n      faceName: '',\n      mapId: '',\n      mapList: [],\n      cmd: '',\n      showEditCmd: false,\n      url: ''\n\n    }\n  },\n  created () {\n    this.url = uploadUrl + 'faceManage/faceUpload'\n    this.getCmd()\n    this.getMap()\n  },\n  methods: {\n    changeCmd (event) {\n      this.cmd = event\n      this.showEditCmd = true\n    },\n    // 修改动作集\n    editCmd () {\n      const formData = this.form.getFieldsValue()\n      if (formData.name == undefined) {\n        this.$message.error('请先输入人脸名称')\n        return\n      }\n      const mapId = formData.mapId == undefined ? 0 : formData.mapId\n      console.log(mapId)\n      this.$refs.cmd.show('编辑人脸动作集', mapId, this.cmd)\n    },\n    // 新增动作集\n    addCmd () {\n      const formData = this.form.getFieldsValue()\n      if (formData.name == undefined) {\n        this.$message.error('请先输入人脸名称')\n        return\n      }\n      const mapId = formData.mapId == undefined ? 0 : formData.mapId\n      this.$refs.cmd.show('编辑人脸动作集', mapId, 0)\n    },\n    add () {\n      this.form.resetFields()\n      this.visible = true\n      this.cmd = ''\n      this.showEditCmd = false\n    },\n    // showCmd(){\n    //   let formData = this.form.getFieldsValue();\n    //   if(formData.name == undefined){\n    //     this.$message.error('请先输入人脸名称')\n    //     return;\n    //   }\n    //   let mapId = formData.mapId == undefined ? 0 : formData.mapId\n    //   this.$refs.cmd.show(formData.name,mapId,this.selectCmd)\n    // },\n    // 编辑动作集后，获取动作集ID\n    getCmdId (cmd) {\n      this.getCmd()\n      this.form.setFieldsValue({\n        cmd: parseInt(cmd)\n      })\n    },\n    getMap () {\n      const org = JSON.parse(localStorage.getItem('user')).orgId\n      queryData({ orgId: org }, '/robotMapLib/getRobotMapLibList').then(res => {\n        console.log('-----------getMap', res)\n        this.mapList = res.result\n      })\n    },\n    getCmd () {\n      const org = JSON.parse(localStorage.getItem('user')).orgId\n      const _this = this\n      queryData({ orgId: org }, '/cmdCollection/getCmdListByOrg').then(res => {\n        console.log(res)\n        if (res.code == 0) {\n          _this.cmdList = res.cmdList\n        } else {\n          _this.$message.error(res.message)\n        }\n      })\n    },\n    handleCancel () {\n      this.previewVisible = false\n    },\n    handlePreview (file) {\n      console.log(file)\n      this.previewImage = file.url || file.thumbUrl\n      this.previewVisible = true\n    },\n    handleChange ({ fileList }) {\n      if (fileList.length > 0) {\n        if (fileList[0].status == 'done') {\n          var res = fileList[0].response\n          console.log(res)\n          if (res.code == 0) {\n            this.fileName = res.fileName\n            this.$message.success(`${res.fileName} 上传成功`)\n          } else {\n            this.$message.error(res.message)\n          }\n        }\n      }\n      this.fileList = fileList\n    },\n    // 下拉框\n    filterOption (input, option) {\n      return option.componentOptions.children[0].text.toLowerCase().indexOf(input.toLowerCase()) >= 0\n    },\n\n    handleSubmit () {\n      const { form: { validateFields } } = this\n      this.confirmLoading = true\n      validateFields((errors, values) => {\n        if (!errors) {\n          var _this = this\n          if (this.fileName == '') {\n            this.$message.error('请上传人脸图片')\n            return\n          }\n          setTimeout(() => {\n            const org = JSON.parse(localStorage.getItem('user')).orgId\n            // values.cmd = this.cmd == '' ? values.cmd : this.cmd\n            addData(Object.assign(values, { org: org, photo: this.fileName }), '/faceManage/addFaceMsg').then(res => {\n              if (res.code == 0) {\n                _this.$message.success('新建成功')\n              } else {\n                _this.$message.error(res.message)\n              }\n              _this.fileList = []\n              _this.visible = false\n              _this.confirmLoading = false\n              _this.$emit('ok', values)\n              // 重置输入框内容\n              _this.form.resetFields()\n            })\n          }, 300)\n        } else {\n          this.confirmLoading = false\n        }\n      })\n    }\n  }\n}\n</script>\n<style>\n  .ant-upload-select-picture-card i {\n    font-size: 32px;\n    color: #999;\n  }\n\n  .ant-upload-select-picture-card .ant-upload-text {\n    margin-top: 8px;\n    color: #666;\n  }\n  .anticon anticon-eye-o{\n      display: none;\n  }\n  .ant-upload-list-item-actions a{\n      display: none;\n  }\n</style>\n"]}]}