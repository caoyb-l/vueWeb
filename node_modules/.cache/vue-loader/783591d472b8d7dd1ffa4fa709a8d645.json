;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsdA;AACA;AACA","file":"Business.vue","sourceRoot":"src/views/ques","sourcesContent":["<template>\r\n  <page-view :title=\"title\">\r\n    <a-card :bordered=\"true\">\r\n      <a-row type=\"flex\">\r\n        <a-col :span=\"5\">\r\n          <div class=\"table-operator\">\r\n            <a-button type=\"primary\" icon=\"plus\" v-action:add @click=\"$refs.createQues.add()\">新建</a-button>\r\n            <a-dropdown v-if=\"selectedRowKeys.length > 0\">\r\n              <a-menu slot=\"overlay\">\r\n                <a-menu-item key=\"1\" v-action:delete @click=\"handleDel()\">\r\n                  <a-icon type=\"delete\" />删除\r\n                </a-menu-item>\r\n              </a-menu>\r\n              <a-button style=\"margin-left: 8px\">\r\n                批量操作\r\n                <a-icon type=\"down\" />\r\n              </a-button>\r\n            </a-dropdown>\r\n          </div>\r\n          <s-table\r\n            bordered\r\n            ref=\"table\"\r\n            size=\"default\"\r\n            rowKey=\"id\"\r\n            :columns=\"columnList\"\r\n            :data=\"loadData\"\r\n            :alert=\"options.alert\"\r\n            :rowSelection=\"options.rowSelection\"\r\n            showPagination=\"auto\"\r\n          >\r\n            <span slot=\"name\" slot-scope=\"text, record\">\r\n              <a-tooltip placement=\"topLeft\" :title=\"record.depict\" arrowPointAtCenter>\r\n                <div ref=\"titleTxt\" @click=\"showList(record.id,record.title)\">{{ text }}</div>\r\n              </a-tooltip>\r\n            </span>\r\n            <span slot=\"action\" slot-scope=\"text, record\">\r\n              <template>\r\n                <a @click=\"handleEditVisible(record)\" v-action:edit>编辑</a>\r\n              </template>\r\n            </span>\r\n          </s-table>\r\n        </a-col>\r\n        <a-col :span=\"1\"></a-col>\r\n        <a-col :span=\"5\" style=\"margin-top:48px\" v-if=\"isShowList\">\r\n          <a-alert :showIcon=\"true\" style=\"margin-bottom: 16px\">\r\n            <template slot=\"message\">\r\n              <span style=\"margin-right: 12px\">\r\n                业务名称:\r\n                <a style=\"font-weight: 600\">{{businessName}}</a>\r\n              </span>\r\n            </template>\r\n          </a-alert>\r\n          <s-table\r\n            bordered\r\n            ref=\"skillTable\"\r\n            size=\"default\"\r\n            rowKey=\"id\"\r\n            :columns=\"columnSkillList\"\r\n            :data=\"loadSkillData\"\r\n            showPagination=\"auto\"\r\n          >\r\n            <span slot=\"keywords\" slot-scope=\"text\">{{ text }}</span>\r\n            <span slot=\"action\" slot-scope=\"text, record\">\r\n              <template>\r\n                <a @click=\"handleDeleteSkillVisible(record)\" v-action:edit>\r\n                  <a-icon type=\"delete\" />\r\n                </a>\r\n\r\n                <a-divider type=\"vertical\" />\r\n\r\n                <a-icon type=\"edit\" @click=\"handleEditSkillVisible(record)\" v-action:edit />\r\n              </template>\r\n            </span>\r\n          </s-table>\r\n          <div style=\"text-align:center;margin:10px;\">\r\n            <a-button type=\"primary\" @click=\"$refs.allSkills.add()\">技能管理</a-button>\r\n          </div>\r\n          <a-textarea\r\n            placeholder=\"请输入基础回答内容\"\r\n            style=\"margin-bottom:10px;margin-top:20px;\"\r\n            :rows=\"2\"\r\n            v-model=\"contents\"\r\n          />\r\n          <a-button type=\"primary\" style=\"width:100%\" @click=\"updateContents\">确定</a-button>\r\n        </a-col>\r\n        <a-col :span=\"1\"></a-col>\r\n        <a-col :span=\"10\" style=\"margin-top:48px\">\r\n            <a-alert :showIcon=\"true\" style=\"margin-bottom: 16px\">\r\n            <template slot=\"message\">\r\n              <span style=\"margin-right: 12px\">\r\n                技能列表:\r\n                <a style=\"font-weight: 600\">{{businessName}} / {{skillName}}</a>\r\n              </span>\r\n            </template>\r\n          </a-alert>\r\n           <a-form :form=\"form\" v-if=\"formVisible\">\r\n      <a-form-item label=\"回答内容\" :labelCol=\"labelCol\" :wrapperCol=\"wrapperCol\">\r\n        <a-textarea\r\n          v-decorator=\"[\r\n            'contents',\r\n            {rules: [{ required: true, message: '请输入回答内容' }],initialValue:editSkills.contents}\r\n          ]\"\r\n          placeholder=\"回答内容\"\r\n          :rows=\"2\"\r\n          autosize\r\n        />\r\n      </a-form-item>\r\n\r\n      <a-form-item label=\"网址信息\" :labelCol=\"labelCol\" :wrapperCol=\"wrapperCol\">\r\n        <a-radio-group\r\n          name=\"radioGroup\"        \r\n            v-decorator=\"[\r\n            'netType',\r\n            {rules: [{ required: true,message: '请选择网址类型' }],initialValue:editSkills.netType}\r\n          ]\"    \r\n        >\r\n          <a-radio :value=\"1\">内网</a-radio>\r\n          <a-radio :value=\"0\">外网</a-radio>\r\n        </a-radio-group>\r\n        <a-input\r\n          placeholder=\"网址信息\"       \r\n          v-decorator=\"[\r\n            'url',\r\n            {rules: [{ message: '请输入网址信息' }],initialValue:editSkills.url}\r\n          ]\"\r\n        />\r\n      </a-form-item>\r\n      <a-form-item v-if=\"editSkills.htmlType==222?true:false\" label=\"上传文件\" :labelCol=\"labelCol\" :wrapperCol=\"wrapperCol\">\r\n        <a-upload\r\n          name=\"file\"\r\n          :multiple=\"true\"\r\n          action=\"http://192.168.1.180:8080/skillsOption/fileUpload\"\r\n          :fileList=\"fileList\"\r\n          :headers=\"headers\"\r\n          :remove=\"handleRemoveFile\"\r\n          @change=\"handleChangeFile\"\r\n          @preview=\"handlePreview\"\r\n        >\r\n          <a-button>\r\n            <a-icon type=\"upload\" />点击上传\r\n          </a-button>\r\n        </a-upload>\r\n      </a-form-item>\r\n       <a-form-item label=\"命令集\" :labelCol=\"labelCol\" :wrapperCol=\"wrapperCol\">      \r\n         <a-button type=\"primary\" @click=\"choiceCmd\">编辑命令集</a-button>      \r\n      </a-form-item>\r\n        <a-form-item style=\"text-align:right\">      \r\n         <a-button type=\"primary\" @click=\"handleFormSubmit\">提交</a-button>      \r\n      </a-form-item>\r\n    </a-form>\r\n        </a-col>\r\n      </a-row>\r\n      <create-ques ref=\"createQues\" @ok=\"handleRefresh\" />\r\n      <edit-ques ref=\"editQues\" @ok=\"handleRefresh\" />\r\n      <all-skills ref=\"allSkills\" :skillsId=\"currectSel\" :skillTableRefresh=\"skillTableRefresh\"></all-skills>\r\n       <a-modal\r\n        title=\"Title\"\r\n        :width=\"1200\"\r\n        :height=\"700\"\r\n        :visible=\"showCmd\"        \r\n    >\r\n     <iframe src=\"https://www.baidu.com/\" style=\"width:100%;height:100%;border:none;\"></iframe>\r\n    </a-modal>\r\n    </a-card>\r\n  </page-view>\r\n</template>\r\n\r\n<script>\r\nimport { PageView } from '@/layouts'\r\nimport { STable } from '@/components'\r\nimport CreateQues from './modules/CreateQues'\r\nimport EditQues from './modules/EditQues'\r\nimport AllSkills from './modules/AllSkills'\r\nimport EditSkills from './modules/EditSkills'\r\nimport { queryData } from '@/api/common'\r\nexport default {\r\n  name: 'TableList',\r\n  components: {\r\n    STable,\r\n    PageView,\r\n    CreateQues,\r\n    EditQues,\r\n    AllSkills,\r\n    EditSkills\r\n  },\r\n  data() {\r\n    return {\r\n       headers: {\r\n        authorization: 'authorization-text'\r\n      },\r\n      skillsList: [],\r\n      isShowList: true,\r\n      currectSel: 0, //当前业务id\r\n      businessName: '',\r\n      skillName:'',\r\n      contents: '',\r\n      contentsId: 0,\r\n      // 表头\r\n      columnList: [\r\n        {\r\n          title: '业务名称',\r\n          dataIndex: 'title',\r\n          scopedSlots: { customRender: 'name' }\r\n        },\r\n        {\r\n          title: '操作',\r\n          dataIndex: 'action',\r\n          scopedSlots: { customRender: 'action' }\r\n        }\r\n      ],\r\n      // 技能表头\r\n      columnSkillList: [\r\n        {\r\n          title: '技能列表',\r\n          dataIndex: 'keywords',\r\n          scopedSlots: { customRender: 'keywords' }\r\n        },\r\n        {\r\n          title: '操作',\r\n          dataIndex: 'action',\r\n          scopedSlots: { customRender: 'action' }\r\n        }\r\n      ],\r\n      loadData: parameter => {\r\n        console.log(parameter)\r\n        let orgId = JSON.parse(localStorage.getItem('user')).orgId\r\n        var param = { descn: 1, orgId: orgId }\r\n        return queryData(Object.assign(parameter, this.queryParam, param), '/skills/getSkillsList').then(res => {\r\n          return res.result\r\n        })\r\n      },\r\n      loadSkillData: parameter => {\r\n        let orgId = JSON.parse(localStorage.getItem('user')).orgId\r\n        var param = { skillsId: this.currectSel }\r\n        return queryData(Object.assign(parameter, this.queryParam, param), '/skillsOption/getSkillsOptionList').then(\r\n          res => {\r\n            return res.result\r\n          }\r\n        )\r\n      },\r\n      selectedRowKeys: [],\r\n      selectedRows: [],\r\n      options: {\r\n        alert: {\r\n          show: true,\r\n          clear: () => {\r\n            this.selectedRowKeys = []\r\n          }\r\n        },\r\n        rowSelection: {\r\n          selectedRowKeys: this.selectedRowKeys,\r\n          onChange: this.onSelectChange\r\n        }\r\n      },\r\n      optionAlertShow: false,   \r\n        labelCol: {\r\n        xs: { span: 24 },\r\n        sm: { span: 7 }\r\n      },\r\n      wrapperCol: {\r\n        xs: { span: 24 },\r\n        sm: { span: 13 }\r\n      }, \r\n      form: this.$form.createForm(this),  \r\n      formVisible:false,   \r\n      showCmd:false,\r\n      fileList:[],\r\n      editSkills: { entityId: '', contents: '', user: '0', netType: '', url: '', org: '1' }\r\n    }\r\n  },\r\n  computed: {\r\n    title() {\r\n      return this.$route.meta.title\r\n    }\r\n  },\r\n  methods: {\r\n    // 打开命令集iframe\r\n    choiceCmd(){\r\n      this.showCmd = true;\r\n    },\r\n    updateContents() {\r\n      let id = JSON.parse(localStorage.getItem('user')).id\r\n      let _this = this\r\n      queryData({ entityId: this.contentsId, contents: this.contents, user: id }, '/skillsOption/updateContents').then(\r\n        res => {\r\n          if (res.code == 0) {\r\n            _this.$message.success('编辑成功')\r\n          } else {\r\n            _this.$message.error(res.message)\r\n          }\r\n        }\r\n      )\r\n    },\r\n    onSelectChange(selectedRowKeys, selectedRows) {\r\n      this.selectedRowKeys = selectedRowKeys\r\n      this.selectedRows = selectedRows\r\n    },\r\n\r\n    showList(id, name) {\r\n      this.businessName = name\r\n      this.currectSel = id\r\n      this.isShowList = true\r\n      //this.showTable(id)\r\n      this.$refs.skillTable.refresh(true)\r\n    },\r\n    skillTableRefresh() {\r\n      console.log('skillTableRefresh')\r\n      this.$refs.skillTable.refresh(true)\r\n    },\r\n    handleRefresh() {\r\n      this.$refs.table.refresh()\r\n    },\r\n\r\n    handleEditVisible(record) {\r\n      this.$refs.editQues.add(record)\r\n    },\r\n    // 删除\r\n    handleDel() {\r\n      var _this = this\r\n      this.$confirm({\r\n        title: '警告',\r\n        content: `真的要删除吗?`,\r\n        okText: '删除',\r\n        okType: 'danger',\r\n        cancelText: '取消',\r\n        onOk() {\r\n          console.log('OK')\r\n          // 在这里调用删除接口\r\n          return new Promise((resolve, reject) => {\r\n            setTimeout(Math.random() > 0.5 ? resolve : reject, 300)\r\n            var id = { entityIds: _this.selectedRowKeys }\r\n            queryData(id, '/skills/delete').then(res => {\r\n              if (res.code == 0) {\r\n                _this.$message.success('删除成功')\r\n                _this.$refs.table.refresh(true)\r\n              } else {\r\n                _this.$message.error(res.message)\r\n              }\r\n            })\r\n          }).catch(err => console.log(err))\r\n        },\r\n        onCancel() {\r\n          console.log('Cancel')\r\n        }\r\n      })\r\n    },\r\n    handleDeleteSkillVisible(record) {\r\n      var _this = this\r\n      this.$confirm({\r\n        title: '警告',\r\n        content: `真的要删除吗?`,\r\n        okText: '删除',\r\n        okType: 'danger',\r\n        cancelText: '取消',\r\n        onOk() {\r\n          console.log('OK')\r\n          // 在这里调用删除接口\r\n          return new Promise((resolve, reject) => {\r\n            setTimeout(Math.random() > 0.5 ? resolve : reject, 300)\r\n            var id = { skillTable: [record.id] }\r\n            queryData(id, '/skillsOption/delete').then(res => {\r\n              if (res.code == 0) {\r\n                _this.$message.success('删除成功')\r\n                _this.$refs.skillTable.refresh()\r\n              } else {\r\n                _this.$message.error(res.message)\r\n              }\r\n            })\r\n          }).catch(err => console.log(err))\r\n        },\r\n        onCancel() {\r\n          console.log('Cancel')\r\n        }\r\n      })\r\n    },\r\n    handleEditSkillVisible(record){\r\n      console.log(record);\r\n      this.skillName=record.keywords;\r\n      this.formVisible=true;\r\n      this.show(record)\r\n    },\r\n\r\n   \r\n    handleChangeFile(info) {    \r\n      let fileList = [...info.fileList]\r\n      // fileList = fileList.slice(-1)\r\n      this.fileList = fileList;\r\n      if (info.file.status == 'uploading') {       \r\n      }\r\n      if (info.file.status === 'done') {\r\n        if(info.file.response.code==0){          \r\n          this.$message.success(`${info.file.name} 上传成功`)\r\n        }else{\r\n          this.fileList.splice(-1,1);\r\n          this.$message.success(`${info.file.response.message}`)\r\n        }\r\n        \r\n      } else if (info.file.status === 'error') {\r\n        this.$message.error(`${info.file.name} 上传失败.`)\r\n      }\r\n    },\r\n     handleRemoveFile(file) {\r\n     // console.log(file);\r\n       let _this=this;       \r\n      let param = { fileUrl: file.response.fileUrl }\r\n      queryData(param, '/skillsOption/delFile').then(res => {\r\n        if (res.code == 0) {   \r\n          console.log(_this.editSkills)     \r\n          // _this.editSkills=res.result;\r\n          // _this.editSkills.fileNameList=res.resultFile;\r\n          return true;\r\n\r\n        } else {\r\n          _this.$message.error(res.message);\r\n          return false;\r\n        }\r\n      })\r\n    },\r\n    handlePreview(file){\r\n     // console.log(file);\r\n      window.open(file.response.fileUrl);\r\n    },\r\n    show(record) {\r\n      let _this=this;\r\n      this.editSkills.fileNameList = []\r\n      this.formVisible = true\r\n      let param = { entityId: record.id }\r\n      queryData(param, '/skillsOption/getSkillsOption').then(res => {\r\n        if (res.code == 0) {        \r\n          _this.editSkills=res.result;\r\n          _this.fileList=res.resultFile;\r\n\r\n        } else {\r\n          _this.$message.error(res.message)\r\n        }\r\n      })\r\n    },\r\n    handleFormSubmit() {\r\n      const {\r\n        form: { validateFields }\r\n      } = this\r\n      this.confirmLoading = true\r\n       var _this = this\r\n      validateFields((errors, values) => {\r\n        if (!errors) {        \r\n          //console.log(values)\r\n             var param = _this.editSkills;\r\n             param.fileNameList=_this.fileList.map((value,index)=>{\r\n               return {\r\n                 fileUrl:value.response.fileUrl,\r\n                 type:value.response.type\r\n               }\r\n             })\r\n            queryData({...param,...values}, '/skillsOption/updateContents').then(res => {\r\n              if (res.code == 0) {\r\n                _this.$message.success('新建成功')\r\n              } else {\r\n                _this.$message.error(res.message)\r\n              }\r\n            \r\n            })\r\n        } else {\r\n          // this.confirmLoading = false\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n<style>\r\n.ant-modal-content{\r\n  height:700px;\r\n}\r\n</style>\r\n"]}]}