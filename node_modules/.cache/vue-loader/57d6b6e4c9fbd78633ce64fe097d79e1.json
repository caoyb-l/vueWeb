ted () {\r\n    this.getPageMeta()\r\n  },\r\n  methods: {\r\n    getPageMeta () {\r\n      // eslint-disable-next-line\r\n      this.pageTitle = (typeof(this.title) === 'string' || !this.title) ? this.title : this.$route.meta.title\r\n      console.log(this.$refs.content)\r\n      const content = this.$refs.content;\r\n      \r\n      if (content) {\r\n        if (content.pageMeta) {\r\n          Object.assign(this, content.pageMeta)\r\n        } else {\r\n          this.description = content.description\r\n          this.linkList = content.linkList\r\n          this.extraImage = content.extraImage\r\n          this.search = content.search === true\r\n          this.tabs = content.tabs\r\n        }\r\n      }\r\n    }\r\n  }\r\n}\r\n",{"version":3,"sources":["PageView.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsDA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"PageView.vue","sourceRoot":"src/layouts","sourcesContent":["<template>\r\n  <div :style=\"!$route.meta.hiddenHeaderContent ? 'margin: -24px -24px 0px;' : null\">\r\n    <!-- pageHeader , route meta :true on hide -->\r\n    <page-header v-if=\"!$route.meta.hiddenHeaderContent\" :title=\"pageTitle\" :logo=\"logo\" :avatar=\"avatar\">\r\n      <slot slot=\"action\" name=\"action\"></slot>\r\n      <slot slot=\"content\" name=\"headerContent\"></slot>\r\n      <div slot=\"content\" v-if=\"!this.$slots.headerContent && description\">\r\n        <p style=\"font-size: 14px;color: rgba(0,0,0,.65)\">{{ description }}</p>\r\n        <div class=\"link\">\r\n          <template v-for=\"(link, index) in linkList\">\r\n            <a :key=\"index\" :href=\"link.href\">\r\n              <a