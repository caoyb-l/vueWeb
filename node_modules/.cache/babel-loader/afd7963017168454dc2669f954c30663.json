: 500;\r\n      margin-bottom: 16px;\r\n    }\r\n\r\n    /deep/ .term {\r\n      color: rgba(0,0,0,.85);\r\n      display: table-cell;\r\n      line-height: 20px;\r\n      margin-right: 8px;\r\n      padding-bottom: 16px;\r\n      white-space: nowrap;\r\n\r\n      &:not(:empty):after {\r\n        content: \":\";\r\n        margin: 0 8px 0 2px;\r\n        position: relative;\r\n        top: -.5px;\r\n      }\r\n    }\r\n\r\n    /deep/ .content {\r\n      color: rgba(0,0,0,.65);\r\n      display: table-cell;\r\n      min-height: 22px;\r\n      line-height: 22px;\r\n      padding-bottom: 16px;\r\n      width: 100%;\r\n      &:empty {\r\n        content: ' ';\r\n        height: 38px;\r\n        padding-bottom: 16px;\r\n      }\r\n    }\r\n\r\n    &.small {\r\n\r\n      .title {\r\n        font-size: 14px;\r\n        color: rgba(0, 0, 0, .65);\r\n        font-weight: normal;\r\n        margin-bottom: 12px;\r\n      }\r\n      /deep/ .term, .content {\r\n        padding-bottom: 8px;\r\n      }\r\n    }\r\n\r\n    &.large {\r\n      /deep/ .term, .content {\r\n        padding-bottom: 16px;\r\n      }\r\n\r\n      .title {\r\n        font-size: 16px;\r\n      }\r\n    }\r\n\r\n    &.vertical {\r\n      .term {\r\n        padding-bottom: 8px;\r\n      }\r\n      /deep/ .term, .content {\r\n        display: block;\r\n      }\r\n    }\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/components/DescriptionList"}]}