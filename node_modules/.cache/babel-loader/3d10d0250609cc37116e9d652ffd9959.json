a-modal\n    title=\"编辑屏幕显示\"\n    style=\"top: 20px;\"\n    :width=\"'50%'\"\n    v-model=\"visible\"\n    @ok=\"handleSubmit\"\n    :destroyOnClose=\"true\">\n    <a-form :form=\"form\">\n      <a-form-item label=\"触发类型\" :labelCol=\"labelCol\" :wrapperCol=\"wrapperCol\">\n        <a-input\n          v-model=\"edit.name\"\n          :disabled=\"true\"\n          placeholder=\"触发类型\"/>\n      </a-form-item>\n      <a-form-item label=\"选择机器人\" :labelCol=\"labelCol\" :wrapperCol=\"wrapperCol\">\n        <a-table\n          ref=\"table\"\n          size=\"middle\"\n          rowKey=\"id\"\n          :alert=\"options.alert\"\n          :rowSelection=\"options.rowSelection\"\n          :columns=\"columns\"\n          :dataSource=\"data\" >\n          <span slot=\"serial\" slot-scope=\"text, record, index\">{{ index + 1 }}</span>\n        </a-table>\n      </a-form-item>\n      <a-form-item label=\"模块区域\" :labelCol=\"labelCol\" :wrapperCol=\"wrapperCol\" v-if=\"descnVisible\">\n        <a-input v-model=\"edit.descn\" placeholder=\"使用英文“,”间隔(不限个数)\" style=\"width: 350px\"/>\n      </a-form-item>\n      <a-form-item label=\"图片\" :labelCol=\"labelCol\" :wrapperCol=\"wrapperCol\" v-if=\"updataVisible\">\n        <a-select\n          showSearch\n          placeholder=\"试着找找已存在的？\"\n          optionFilterProp=\"children\"\n          style=\"width: 200px\"\n          :filterOption=\"filterOption\"\n          @change=\"handleAppImageChange2\"\n          v-decorator=\"[\n            'fileName',\n            {rules: [{required: true, message: '请添加图片' }],initialValue:edit.fileName}\n          ]\"\n        >\n          <a-select-option v-for=\"(item) in imageList\" :key=\"item.fileName\" :value=\"item.fileName\" >{{ item.name }}</a-select-option>\n        </a-select>\n      </a-form-item>\n      <a-form-item label=\"名称\" :labelCol=\"labelCol\" :wrapperCol=\"wrapperCol\">\n        <a-input v-model=\"edit.name\" placeholder=\"<行行好，取个名(～￣▽￣)～><非必填>😘\" style=\"width: 350px\"/>\n      </a-form-item>\n      <a-form-item label=\"重新上传\" :labelCol=\"labelCol\" :wrapperCol=\"wrapperCol\" v-if=\"updataVisible\">\n        <a-upload\n          :action= \"url\"\n          listType=\"picture-card\"\n          :fileList=\"fileList\"\n          :remove=\"handleRemoveFile\"\n          @preview=\"handlePreview\"\n          @change=\"handleChange\"\n        >\n          <div v-if=\"fileList.length < 1\">\n            <a-icon type=\"plus\" />\n            <div class=\"ant-upload-text\">Upload</div>\n          </div>\n        </a-upload>\n        <a-modal :visible=\"appImageVisible\" :footer=\"null\" @cancel=\"handleCancel\">\n          <img alt=\"example\" style=\"width: 100%\" :src=\"appImageWebUrl\" />\n        </a-modal>\n      </a-form-item>\n    </a-form>\n  </a-modal>\n</template>\n\n<script>\nimport { queryData, addData } from '@/api/common'\nimport { STable } from '@/components'\nimport { uploadUrl } from '@/api/upload'\nimport { constants } from 'fs'\nexport default {\n  components: {\n    STable\n  },\n  data () {\n    return {\n      edit: { id: 0, name: '', fileName: '', imageName: '', descn: '' },\n      labelCol: {\n        xs: { span: 24 },\n        sm: { span: 7 }\n      },\n      wrapperCol: {\n        xs: { span: 24 },\n        sm: { span: 13 }\n      },\n      visible: false,\n      appImageVisible: false,\n      appImageWebUrl: '', // 图片访问地址\n      descnVisible: false,\n      updataVisible: false,\n      fileList: [],\n      confirmLoading: false,\n      form: this.$form.createForm(this),\n      queryParam: {},\n      imageList: [{\n        'fileName': '123456.png',\n        'name': '党建待机（创创）'\n      }, {\n        'fileName': '123457.png',\n        'name': '教育待机（创创）'\n      }, {\n        'fileName': '123458.png',\n        'name': '医疗待机（创创）'\n      }, {\n        'fileName': '123459.png',\n        'name': '政务待机（创创）'\n      }],\n      // 表头\n      columns: [\n        {\n          title: '#',\n          dataIndex: 'no',\n          scopedSlots: { customRender: 'serial' }\n        },\n\n        {\n          title: '机器人名称',\n          dataIndex: 'robotName',\n          scopedSlots: { customRender: 'robotName' }\n        },\n        {\n          title: '机器人编码',\n          dataIndex: 'code',\n          scopedSlots: { customRender: 'code' }\n        }\n      ],\n      // 加载数据方法 必须为 Promise 对象\n      data: [],\n      options: false,\n      optionAlertShow: false,\n      url: ''\n    }\n  },\n  created () {},\n  methods: {\n    add (record) {\n      console.log(record)\n      this.visible = true\n      if (record.type == '主页') {\n        this.descnVisible = true\n        this.updataVisible = false\n        this.edit.descn = record.descn\n      } else {\n        this.updataVisible = true\n        this.descnVisible = false\n      }\n      this.edit.id = record.id\n      this.edit.fileName = record.fileName\n      this.edit.imageName = record.name\n      this.edit.name = record.name\n      this.data = []\n      this.data.push(record)\n      this.url = uploadUrl + 'appImage/fileUpload'\n    },\n    filterOption (input, option) {\n      return option.componentOptions.children[0].text.toLowerCase().indexOf(input.toLowerCase()) >= 0\n    },\n    handleSubmit () {\n      const {\n        form: { validateFields }\n      } = this\n      var _this = this\n      this.confirmLoading = true\n      validateFields((errors, values) => {\n        if (!errors) {\n          if (this.edit.name == '' || this.edit.name == null) {\n            this.$message.error('懒得名称都不写了？😈')\n            return\n          }\n          var param = {\n            robotId: [],\n            name: this.edit.name,\n            descn: this.edit.descn,\n            entityId: _this.edit.id,\n            fileName: this.edit.fileName\n          }\n          queryData({ ...param, ...values }, '/appImage/save').then(res => {\n            if (res.code == 0) {\n              _this.$message.success('修改成功')\n              this.edit = { id: 0, name: '', fileName: '', imageName: '', descn: '' }\n            } else {\n              _this.$message.error(res.message)\n            }\n            _this.visible = false\n            _this.confirmLoading = false\n            _this.$emit('ok', values)\n            // 重置输入框内容\n            _this.form.resetFields()\n          })\n        } else {\n          this.confirmLoading = false\n        }\n      })\n    },\n    handleCancel () {\n      this.appImageVisible = false\n    },\n    // 公共图片选择列表监听事件\n    handleAppImageChange2 (value) {\n      this.edit.fileName = value\n    },\n    // 文件预览\n    handlePreview (file) {\n      console.log(file.url + '/' + file.thumbUrl)\n      this.appImageWebUrl = file.url || file.thumbUrl\n      this.appImageVisible = true\n    },\n    // 删除文件\n    handleRemoveFile (file) {\n      const _this = this\n      queryData({ fileName: file.response.fileName }, '/appImage/delFile').then(res => {\n        if (res.code == 0) {\n          _this.$message.error(res.message)\n          return true\n        } else {\n          _this.$message.error(res.message)\n          return false\n        }\n      })\n    },\n    // 文件长传监听\n    handleChange ({ fileList }) {\n      if (fileList.length > 0) {\n        if (fileList[0].status == 'done') {\n          var res = fileList[0].response\n          console.log(res)\n          if (res.code == 0) {\n            this.edit.fileName = res.fileName\n            this.$message.success(`${res.fileName} 上传成功`)\n          } else {\n            this.$message.error(res.message)\n          }\n        }\n      }\n      this.fileList = fileList\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/robot/modules"}]}